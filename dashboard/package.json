{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev && pnpm run minify && pnpm run minify-css",
    "build": "next build && pnpm minify && pnpm run minify-css",
    "start": "next start",
    "lint": "next lint && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "cd .. && husky install dashboard/.husky",
    "pre-commit-lint": "pnpm lint-staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-coverage": "jest --coverage",
    "test": "jest",
    "minify": "terser .next/**/*.js -o .next/**/*.js  --compress --mangle --format",
    "postinstall": "patch-package",
    "minify-css": "cleancss -o .next/static/css/**/*.css .next/static/css/**/*.css"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@firebase/auth": "^1.5.1",
    "@milahu/patch-package": "^6.4.14",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.3",
    "dayjs": "^1.11.10",
    "firebase": "^10.7.1",
    "framer-motion": "^10.16.16",
    "next": "14.0.4",
    "react": "^18",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.12",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "^7.49.2",
    "react-intersection-observer": "^9.8.0",
    "react-quill": "^2.0.0",
    "terser": "^5.27.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@chakra-ui/storybook-addon": "^5.1.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@storybook/addon-essentials": "7.6.7",
    "@storybook/addon-interactions": "7.6.7",
    "@storybook/addon-links": "7.6.7",
    "@storybook/addon-onboarding": "1.0.10",
    "@storybook/blocks": "7.6.7",
    "@storybook/nextjs": "7.6.7",
    "@storybook/react": "7.6.7",
    "@storybook/test": "7.6.7",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "clean-css-cli": "^5.6.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-next-dynamic": "^1.0.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "storybook": "7.6.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "npx prettier --write",
      "eslint app/*.{js,jsx,ts,tsx} --fix-dry-run --no-error-on-unmatched-pattern"
    ]
  }
}
